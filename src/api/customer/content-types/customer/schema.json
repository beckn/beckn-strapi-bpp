{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "contact": {
      "type": "biginteger"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating",
      "mappedBy": "customer"
    }
  }
}
